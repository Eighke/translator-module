{% macro macro(keys) %}

    {% import _self as self %}

    {% for key, value in keys %}
        {% if value is iterable %}
    '{{ key }}' => [
        {{ self.macro(value) }}
    ],
        {% else %}
    '{{ key }}' => '{{ value|e }}',
        {% endif %}
    {% endfor %}
{% endmacro %}

return [{{ self.macro(require) }}];
