{% macro macro(keys) %}
    {% for key, value in keys %}
        {% if value is iterable %}
    '{{ key }}' => [
        {{ _self.macro(value) }}
    ],
        {% else %}
    '{{ key }}' => '{{ value|e }}',
        {% endif %}
    {% endfor %}
{% endmacro %}

{% import _self as do %}

return [{{ do.macro(require) }}];
