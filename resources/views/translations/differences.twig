<div class="modal-header">
    <button class="close" data-dismiss="modal">
        <span>&times;</span>
    </button>
    <h4 class="modal-title">{{ trans('anomaly.module.translator::message.difference') }}</h4>
</div>

<div class="modal-body">
    {% if missing %}
        <ul class="nav nav-pills nav-stacked">
            {% for file, keys in missing %}
                <li class="nav-item">
                    <a href="/admin/translator/translate/{{ addon.namespace }}" class="nav-link">
                        <strong>{{ file }}</strong>
                        <br>
                        <small>
                            {% for key in keys %}
                                <span class="label label-warning">{{ key }}</span>
                            {% endfor %}
                        </small>
                    </a>
                </li>
            {% endfor %}
        </ul>
    {% else %}
        {{ trans('streams::message.no_results') }}
    {% endif %}
</div>
